}

.myorder-t {
  width: 100%;
  line-height: 88rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
  font-size: 32rpx;
  color: #999;
  margin-bottom: 43rpx;
  border-bottom: solid 3rpx #f6f6f6;
}

.myorderactice {
  color: #ff2740;
  border-bottom: 4rpx solid #ff2740;
}

.coupon1 {
  width: 100%;
  margin-top: 200rpx;
  text-align: center;
}

.coupon1 image {
  width: 310rpx;
  height: 243rpx;
}

.coupon1t {
  margin-top: 10rpx;
