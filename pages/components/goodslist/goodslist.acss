.info-bottom {
  background: #fff;
  margin-top: 19rpx;
  overflow: hidden;
  padding-bottom: 30rpx;
}

.info-bottom-t {
  height: 63rpx;
  margin: 27rpx 0 0 46rpx;
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
  font-weight: 600;
  line-height: 40rpx;
}

.info-bottom-m {
  margin: 20rpx 32rpx 0;
  display: flex;
  border-bottom: 2rpx solid rgba(236, 236, 236, 1);
  padding-bottom: 20rpx;
}

.info-bottom-m:nth-last-of-type(1) {
  border-bottom: none;
}

.info-bottom-ml {
  padding: 12rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 8rpx;
  background: #fff;
}

.info-bottom-ml image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 8rpx;
}

.info-bottom-mrb {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.info-bottom-mrbr image {
  width: 50rpx;
  height: 50rpx;
}

.info-bottom-mr {
  margin-left: 31rpx;
}

.info-bottom-mrt {
  margin-top: 12rpx;
  width: 450rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
  font-weight: 500;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  letter-spacing: 2rpx;
}

.info-bottom-mrm {
  margin-top: 6rpx;
  width: 450rpx;
  line-height: 33rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  font-weight: 400;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  letter-spacing: 2rpx;
}

.info-bottom-mrb {
  margin-top: 28rpx;
}

.info-bottom-mrbl {
  font-size: 38rpx;
  color: rgba(249, 67, 11, 1);
  font-weight: 500;
}

.info-bottom-mrbl text {
  font-size: 28rpx;
}

.bottom_tips {
  background: #fff;
}
