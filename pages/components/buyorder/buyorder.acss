.store {
    margin-top: 20rpx;
}

checkbox {
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    color: white;
}

.a-checkbox-checked {
    width: 30rpx;
    height: 30rpx;
    background: #ee1525;
    text-align: center;
    color: white;
    border: #ee1525 2rpx solid;
}

.a-icon-svg {
    width: 20rpx !important;
    height: 20rpx !important;
}

checkbox .a-icon-success_no_circle {
    fill: white;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0rpx;
    margin: auto;
}

/* add_box */

.add_box {
    width: 710rpx;
    margin: 20rpx auto 0;
}

.add_top {
    display: flex;
    align-items: flex-end;
}

.add_top > view {
    width: 50%;
    height: 62rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 16rpx 16rpx 0px 0px;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    display: flex;
    align-items: center;
    justify-content: center;
}

.add_info {
    width: 100%;
    height: 244rpx;
    background: url('/images/icon/locationAdd.png') no-repeat;
    background-size: 100% 100%;
    box-sizing: border-box;
    padding-left: 30rpx;
    padding-top: 20rpx;
    position: relative;
}

.add_store_Km {
    font-size: 20rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(51, 51, 51, 1);
    width: 144rpx;
    height: 36rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 4rpx;
    position: absolute;
    right: 40rpx;
    top: 81rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.add_top_active {
    height: 72rpx !important;
    background: rgba(255, 39, 64, 1) !important;
    font-size: 30rpx !important;
    font-weight: 500 !important;
    color: rgba(255, 255, 255, 1) !important;
}

.add_store_name {
    font-size: 34rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: rgba(34, 34, 34, 1);
}

.add_store_add {
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    margin-top: 20rpx;
}

.add_store_num {
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(255, 39, 64, 1);
    margin-top: 10rpx;
}

.add_store_time {
    font-size: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(102, 102, 102, 1);
    position: absolute;
    left: 30rpx;
    bottom: 36rpx;
}


