page {
    box-sizing: border-box;
    /*padding-top: 35rpx;*/
    padding-bottom: 160rpx;
}

.address {
    width: 710rpx;
    height: 150rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
    z-index: 1;
    border-radius: 16rpx;
    margin: 0 auto;
}

.address2 {
    border-radius: 0 0 16rpx 16rpx;
}

.address-l {
    margin-left: 24rpx;
    display: flex;
    align-items: center;
}

.address-lr {
    margin-left: 15rpx;
}

.address-lrt {
    font-size: 26rpx;
    color: #999999;
    line-height: 45rpx;
}

.address-lrb {
    display: flex;
    align-items: center;
}

.address-lrb text {
    display: block;
    font-size: 34rpx;
    color: #000000;
    line-height: 45rpx;
    width: 500rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.address-lrb image {
    display: block;
    width: 16rpx;
    height: 20rpx;
    margin-right: 6rpx;
}

.address-pos {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
}

.address-pos image {
    width: 100%;
    height: 6rpx;
}


.address-r {
    margin-right: 24rpx;
}

.address-r image {
    width: 24rpx;
    height: 24rpx;
}

.address-default {
    display: inline-block;
    width: 66rpx;
    height: 30rpx;
    background-color: #ff2740;
    border-radius: 2rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fff;
}


.goodsinfo {
    background: #fff;
    width: 710rpx;
    border-radius: 10rpx 10rpx 0 0;
    margin: 0 auto;
}

.goodsinfo-t {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding-left: 30rpx;
    height: 76rpx;
}

.goodsinfo-tr image {
    width: 32rpx;
    height: 32rpx;
}

.goodsinfo-tr text {
    margin-left: 10rpx;
    font-size: 28rpx;
    color: #333;
}

.goodsinfo-tr {
    display: flex;
    align-items: center;
}

.goodsinfo-m {
    margin-top: 4rpx;
    padding-bottom: 20rpx;
}

.coupon-l {
    font-size: 28rpx;
    font-weight: 400;
