/**/
.guangbo image {
    width: 34rpx;
    height: 34rpx;
    margin-right: 10rpx;
}

.guangbo {
    width: 100%;
    height: 80rpx;
    display: flex;
    align-items: center;
    z-index: 100;
    background: rgba(253, 235, 221, 0.6);
    box-sizing: border-box;
    padding: 0 20rpx;
    margin-bottom: 20rpx;
}

.guangbo > view {
    width: 90%;
    overflow: hidden;
    white-space: nowrap;
    position: relative;
}

.guangbo_info {
    white-space: nowrap;
    width: 100%;
    color: #DB6D2A;
    animation: 10s wordsLoop linear infinite normal;
    font-size: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
}

@keyframes wordsLoop {
    0% {
        transform: translateX(10px);
        -webkit-transform: translateX(10px);
    }
    100% {
        transform: translateX(-120%);
        -webkit-transform: translateX(-120%);
    }
}
